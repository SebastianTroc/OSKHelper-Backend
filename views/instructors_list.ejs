<% include header.ejs %>

    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>

    <% if (data.instructors.length) { %>
    <ul>
    	<% data.instructors.forEach(function(instructor){ %>
    		<li>
    			<a href="/instructors/<%= instructor._id %>"><%= instructor.name %></a> (<%= instructor %>)
    		</li>
    	<% }) %>
    </ul>
    <% } else { %>
        Jeszcze nie ma żadnych instruktorów w bazie.
    <% } %>

<% include footer.ejs %>